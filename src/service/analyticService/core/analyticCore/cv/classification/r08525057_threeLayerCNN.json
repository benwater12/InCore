{
    "dataType":"cv",
    "projectType":"classification",
    "algoName":"r08525057_threeLayerCNN",
    "description": "Three layers convolutional neural network.",
    "lib":"keras",
    "param":[
        {
            "name":"hidden_neuron",
            "description":"number of neurons (filters of Convolutional Layer)",
            "type":"int",
            "lowerBound":2,
            "upperBound":128,
            "default":32
        },
        {
            "name":"hidden_kernel_size",
            "description":"size of kernel of Convolutional Layer)",
            "type":"int",
            "lowerBound":2,
            "upperBound":7,
            "default":3
        },
        {
            "name":"hidden_activation",
            "description":"activation function",
            "type":"enum",
            "list":["linear","tanh","relu","elu","softmax","selu"],
            "default":"relu"
        },
        {
            "name":"dropout",
            "description":"cut off some percentage of neurons",
            "type":"float",
            "lowerBound":0.05,
            "upperBound":0.9,
            "default":0.5
        },
        {
            "name":"optimizer",
            "description":"optimizer",
            "type":"enum",
            "list":["sgd","rmsprop","adagrad","adam"],
            "default":"adam"
        },
        {
            "name":"epochs",
            "description":"train epochs",
            "type":"int",
            "lowerBound":5,
            "upperBound":200,
            "default":100
        },
        {
            "name":"batch_size",
            "description":"batch size of training",
            "type":"int",
            "lowerBound":4,
            "upperBound":128,
            "default":32
        }
    ],
    "input":[
        {
            "name": "X",
            "description": "input data",
            "type":"path",
            "amount":"single"
        }
    ],
    "output":[
        {
            "name": "Y",
            "description": "output data",
            "type":"classifiable"
        }
    ]
}